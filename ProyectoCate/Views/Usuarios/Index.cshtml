@model IEnumerable<ProyectoCate.Usuarios>

@{
    ViewBag.Title = "Usuarios Registrados";
}

<h2>Usuarios Registrados</h2>

<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Correo)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Correo)
        </td>
        <td>
            @Html.ActionLink("Editar", "Edit", new { id=item.UsuarioID }) |
            @Html.ActionLink("Detalles", "Details", new { id=item.UsuarioID }) |
            @Html.ActionLink("Eliminar", "Delete", new { id=item.UsuarioID })
        </td>
    </tr>
}

</table>
<p>
    @Html.ActionLink("Crear Usuario", "Registrar", "Acceso")
</p>
